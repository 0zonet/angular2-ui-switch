{
  "name": "angular2-ui-switch",
  "version": "1.0.1",
  "description": "switch button for angular2",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --config webpack.config.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base",
    "typings": "typings install",
    "clean": "rimraf ./dist",
    "tsc": "tsc --project . --declaration",
    "tsc-single": "tsc --project . --rootDir ./src --module system --outFile ./dist/angular2-ui-switch.js",
    "uglify": "uglifyjs ./dist/angular2-ui-switch.js --output ./dist/angular2-ui-switch.min.js",
    "build": "npm run clean && npm run tsc && npm run tsc-single && npm run uglify",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyang041060120/angular2-ui-switch.git"
  },
  "author": "yuyangyang",
  "license": "MIT",
  "keywords": [
    "angular2",
    "uiswitch",
    "switch",
    "ui"
  ],
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/core": "2^.0.0-rc.1",
    "@angular/http": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "angular2-in-memory-web-api": "^0.0.10",
    "bootstrap": "~3.3.x",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.5.2",
    "rxjs": "5.0.0-beta.6",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "uglify-js": "^2.6.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "zone.js": "^0.6.12"
  },
  "peerDependencies": {
    "@angular/common": "^2.0.0-rc.1",
